{
    "name": "storefront_backend",
    "version": "0.1.0",
    "description": "This is the backend of the storefront project",
    "main": "server.ts",
    "scripts": {
        "start": "nodemon src/server.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
        "test": "ENV=test && db-migrate --env test up && jasmine-ts && db-migrate reset --env test",
        "dev_up": "ENV=dev db-migrate --env dev up",
        "dev_down": "ENV=dev db-migrate db:drop dev",
        "tsc": "tsc"
    },
    "author": "Udacity",
    "license": "ISC",
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.9",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/pg": "^7.14.7",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "db-migrate": "^0.11.12",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-jwt": "^6.0.0",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^2.0.4",
        "jwt-simple": "^0.5.6",
        "pg": "^8.5.1",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.1.6",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@types/jasmine": "^3.8.2",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "jasmine": "^3.8.0",
        "jasmine-spec-reporter": "^6.0.0",
        "jasmine-ts": "^0.3.0",
        "nodemon": "^2.0.12",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9"
    }
}
